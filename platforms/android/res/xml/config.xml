<?xml version='1.0' encoding='utf-8'?>
<widget id="com.vista.Caserevision" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <name>Caserevision</name>
    <description>
        Caserevision application.
    </description>
    <author email="Vistadev@yandex.ru" href="http://cordova.io">
        VistaDev
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="phonegap-version" value="3.6.3" />
    <gap:splash gap:platform="android" src="res/screen/android/splash.png" />
    <gap:splash gap:platform="ios" src="res/screen/ios/splash.png" />
    <gap:plugin name="org.apache.cordova.splashscreen" />
    <gap:splash gap:platform="android" src="res/screen/splash.png" />
    <gap:splash gap:platform="ios" src="res/screen/splash.png" />
    <splash src="res/screen/splash.png" />
    <gap:splash src="res/screen/splash.png" />
    <feature name="WebIntent">
        <param name="android-package" value="com.borismus.webintent.WebIntent" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
</widget>
